                      children: [/*#__PURE__*/_jsx(\"stop\", {\n                        \"stop-color\": \"white\"\n                      }), /*#__PURE__*/_jsx(\"stop\", {\n                        offset: \"0.79478\",\n                        \"stop-color\": \"white\",\n                        \"stop-opacity\": \"0\"\n                      })]\n                    })\n                  })]\n                })]\n              }), /*#__PURE__*/_jsx(CarouselItemText, {\n                children: item.text\n              })]\n            })\n          }, index);\n        })\n      })\n    }), /*#__PURE__*/_jsx(CarouselButtons, {\n      children: TimeLineData.map(function (item, index) {\n        return /*#__PURE__*/_jsx(CarouselButton, {\n          index: index,\n          active: activeItem,\n          onClick: function onClick(e) {\n            return handleClick(e, index);\n          },\n          type: \"button\",\n          children: /*#__PURE__*/_jsx(CarouselButtonDot, {\n            active: activeItem\n          })\n        }, index);\n      })\n    }), /*#__PURE__*/_jsx(SectionDivider, {})]\n  });\n};\n\nexport default Timeline;","map":null,"metadata":{},"sourceType":"module"}